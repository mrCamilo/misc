{
  "manifest_version": 2,
  "name": "Hide Likes",
  "version": "1.0",

  "description": "Hides the likes and dislikes on Youtube videos.",

  "icons": {
    "49": "icons/border-49.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["hidelikes.js"]
    }
  ]
}
